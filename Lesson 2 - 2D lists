
shoe_rack = []

shoe_rack.append(["trainers", "sandals"])
shoe_rack.append(["brown shoes","sneakers"])
shoe_rack.append(["red pumps","soccer shoes"])

print(shoe_rack)

print(len(shoe_rack))

row = shoe_rack[0]
print(len(row))

shoe_rack.insert(1,["ballet shoes", "red high heels"])
print(shoe_rack)

shoe_rack = shoe_rack[0:2]
print(shoe_rack)

shoe_rack = shoe_rack[-2]
print(shoe_rack)

shoe_rack = shoe_rack[:2]
shoe_rack = shoe_rack[0:]
print(shoe_rack)

if len(shoe_rack) > 2: 
    shoe_rack.pop(2)
    print(shoe_rack)
else: 
    print("There is nothing to pop at index 2")

if ["ballet shoes", "red high heels"] in shoe_rack: 
    shoe_rack.remove(["ballet shoes", "red high heels"])
    print(shoe_rack)
else: 
    print("Item cannot be found within the shoe rack") 

shoe_rack.append(["trainers", "sandals"])
shoe_rack.append(["brown shoes","sneakers"])
shoe_rack.append(["red pumps","soccer shoes"])

shoe_rack = shoe_rack[0:1] + shoe_rack[-1]
print(shoe_rack)

shoe_rack = [["brown shoes", "sneakers", "black high heels"],
            ["blue converse", "sandals", "ballet shoes "], 
            ["red pumps", "soccer shoes" "red high heels"]] 

shoe_rack.sort()
print(shoe_rack)

shoe_rack.reverse()
print(shoe_rack)

